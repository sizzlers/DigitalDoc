<html>

<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.4/angular.min.js"></script>

 <script src="https://rawgithub.com/gsklee/ngStorage/master/ngStorage.js"></script>
    <script src="styles/js/jquery.min.js"></script> 



<div ng-app="myApp" ng-controller="myCtrl">

<input type="text"  ng-model="txt1"> <br>
<input type="text"  ng-model="txt2"><br>

<br>
sum {{sum}}
{{data}}
 <button ng-click="AddNumbers()">hello</button>
  <button ng-click="save()">Save</button>
   <button ng-click="retrieve()">get</button>
      <button ng-click="clear()">clear</button>
</div>

<script>
var app = angular.module('myApp', [ 'ngStorage']);
app.controller('myCtrl', function($scope,storageService) {
      
     $scope.AddNumbers = function() {
            var a = Number($scope.txt1 || 0);
            var b = Number($scope.txt2 || 0);
            $scope.sum = a+b;
        }
		
		$scope.saved = localStorage.getItem('locator');
		$scope.locator = (localStorage.getItem('locator')!==null) ? JSON.parse($scope.saved) : [ {text: 'test', visited: false}];
		storageService.set('locator', JSON.stringify($scope.locator));
		
		$scope.save = function() {
		
		var locArr = $scope.locator;
		angular.forEach(locArr, function(data){
				if(locArr.indexOf(data.text) !== $scope.txt1){
					$scope.locator.push({
						text: $scope.txt1,
						visited: true
						});
				}
			});
				
		
		storageService.set('locator', JSON.stringify($scope.locator));
            
			var a = $scope.txt1
            var b ="";
            storageService.set('1', a);

        }
		$scope.retrieve = function() {
        
			//$scope.data = storageService.get('1')
		
			var count = 0;
			angular.forEach($scope.locator, function(data){
				count+= data.text ;//? 0 : 1;
			});
			$scope.data = count;
	

        }
		$scope.clear = function() {
        
			//$scope.data = storageService.get('1')
		
			 storageService.remove();

        }
});

app.factory('storageService', ['$rootScope', function($rootScope) {

    return {
        get: function(key) {
            return localStorage.getItem(key);
        },
        set: function(key, data) {
            localStorage.setItem(key, data);
        },
		remove:function(){
		localStorage.clear();
		}
    };
}]);
</script>
</html>
